{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "string"
    },
    "cars": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "name": {
              "type": "string"
            },
            "price": {
              "type": "string"
            },
            "image": {
              "type": "string"
            }
          }
        }
      ]
    }
  },
  "required": [
    "status",
    "cars"
  ]
}