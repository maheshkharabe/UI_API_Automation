<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="ExpandTesting_UI" parallel = "methods" thread-count="1">
    <groups>
        <run>
            <include name="SANITY"/>
            <include name="REGRESSION"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="utils.TestListener"/>
    </listeners>
    <test name="TestCarsUI">
        <classes>
            <class name="utils.TestConfigSetUp"/>
            <class name="tests.ui.TestCarsUI"/>
        </classes>
    </test>
</suite>